message format
json: {
    cmd: string
    msg: object
}

cmd (from judgecenter):
    judge: { // send submission
        submission_id: integer,
        verdict_id: integer,
        execute_type_id: integer,
        testdata: [
            {
                id: integer,
                time: integer,
                memory: integer,
            },
        ]
    }
    type: // return type
        integer


cmd (from judge):
    judged: { // return meta of submission
        submission_id: integer,
        testdata: [
            {
                id: integer,
                time: integer,
                memory: integer,
                verdict: integer
            },
        ] 
    }
    type: // get type
        None
    type: // set type
        integer
    token: // auth
        string
